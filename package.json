{
  "name": "ide-cpp",
  "main": "./lib/main",
  "version": "0.0.3",
  "description": "C++ and C language support for Atom-IDE",
  "repository": "https://github.com/thomasjo/atom-ide-cpp",
  "license": "MIT",
  "keywords": [
    "c++",
    "c",
    "ide",
    "clangd"
  ],
  "engines": {
    "atom": ">=1.21.0"
  },
  "dependencies": {
    "atom-languageclient": "^0.6.3"
  },
  "devDependencies": {
    "snazzy": "^7.0.0",
    "standard": "^10.0.3"
  },
  "standard": {
    "globals": [
      "afterEach",
      "atom",
      "beforeEach",
      "describe",
      "expect",
      "it",
      "jasmine",
      "runs",
      "spyOn",
      "waitsFor",
      "waitsForPromise"
    ]
  },
  "enhancedScopes": [
    "source.c",
    "source.cpp"
  ],
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    },
    "datatip": {
      "versions": {
        "0.1.0": "consumeDatatip"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    },
    "code-actions": {
      "versions": {
        "0.1.0": "provideCodeActions"
      }
    },
    "code-format.range": {
      "versions": {
        "0.1.0": "provideCodeFormat"
      }
    },
    "code-highlight": {
      "versions": {
        "0.1.0": "provideCodeHighlight"
      }
    },
    "definitions": {
      "versions": {
        "0.1.0": "provideDefinitions"
      }
    },
    "find-references": {
      "versions": {
        "0.1.0": "provideFindReferences"
      }
    },
    "signature-help": {
      "versions": {
        "0.1.0": "provideSignatureHelp"
      }
    },
    "outline-view": {
      "versions": {
        "0.1.0": "provideOutlines"
      }
    }
  },
  "configSchema": {
    "clangdPath": {
      "type": "string",
      "default": "/usr/bin/clangd"
    }
  }
}
